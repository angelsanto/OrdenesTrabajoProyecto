@model ENTIDAD.DEPARTAMENTO

@{
    ViewBag.Title = "detalledepa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalle Departamento</h2>

<div>
    <h4>DEPARTAMENTO</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label>NUMERO IDENTIFICADOR</label>       
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ID_DEPARTA)
        </dd>
        <dt>
            <label>DEPARTAMENTO</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DEPARTAMENTO1)
        </dd>
    </dl>
</div>
<p>
  
    <a class="btn btn-primary" style="border-bottom:double" href="@Url.Action("editar","Departamento",new {id = Model.ID_DEPARTA})"><i class="glyphicon glyphicon-edit"></i> Editar</a> 
    <a class="btn btn-danger" style="border-bottom:double" href="@Url.Action("Index","Departamento")"><i class="glyphicon glyphicon-remove"></i> Cancelar</a>
</p>
